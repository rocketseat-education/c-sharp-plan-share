<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <Style ApplyToDerivedTypes="True" TargetType="ContentPage">
        <Setter Property="Padding" Value="30,10,30,40" />
        <Setter Property="BackgroundColor" Value="{AppThemeBinding Dark={StaticResource PAGE_BACKGROUND_COLOR_DARK}, Light={StaticResource PAGE_BACKGROUND_COLOR_LIGHT}}" />
    </Style>
</ResourceDictionary>